{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AzureBIDev"
		},
		"ADFBIDEVOnPrem_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADFBIDEVOnPrem'"
		},
		"ADFCycles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ADFCycles'"
		},
		"OnPremfileLinkService_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremfileLinkService'"
		},
		"linkserviceADFDevStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkserviceADFDevStorage'"
		},
		"LS_AzureBIDEVBlobStorage_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "sec-azurebidevstorage-connectionString"
		},
		"LS_KV_AzureBIDEV_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://RGBIDEV.vault.azure.net/"
		},
		"OnPremfileLinkService_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\PowerBI\\"
		},
		"OnPremfileLinkService_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "LAPTOP-JO04ITQ5\\Amit Rathore"
		},
		"ADFSQLDB_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/344d0f8e-c80e-4498-9bda-0941b8ffccad/resourceGroups/AzureBIDev/providers/Microsoft.Sql/servers/adfbidev"
		},
		"ADFSQLDB_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AzureBIDev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADFBIDEVOnPrem_connectionString": {
						"value": "[parameters('ADFBIDEVOnPrem_connectionString')]"
					},
					"OnPremfileLinkService_password": {
						"value": "[parameters('OnPremfileLinkService_password')]"
					},
					"ADFCycles_connectionString": {
						"value": "[parameters('ADFCycles_connectionString')]"
					},
					"linkserviceADFDevStorage_connectionString": {
						"value": "[parameters('linkserviceADFDevStorage_connectionString')]"
					},
					"LS_KV_AzureBIDEV_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KV_AzureBIDEV_properties_typeProperties_baseUrl')]"
					},
					"OnPremfileLinkService_properties_typeProperties_host": {
						"value": "[parameters('OnPremfileLinkService_properties_typeProperties_host')]"
					},
					"OnPremfileLinkService_properties_typeProperties_userId": {
						"value": "[parameters('OnPremfileLinkService_properties_typeProperties_userId')]"
					},
					"ADFSQLDB_properties_privateLinkResourceId": {
						"value": "[parameters('ADFSQLDB_properties_privateLinkResourceId')]"
					},
					"ADFSQLDB_properties_groupId": {
						"value": "[parameters('ADFSQLDB_properties_groupId')]"
					},
					"LS_AzureBIDEVBlobStorage_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_AzureBIDEVBlobStorage_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "AzureBIDev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'AzureBIDev_ArmTemplate_0')]"
			]
		}
	]
}